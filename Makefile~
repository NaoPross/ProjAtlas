compile_nao:
	javac -d bin -sourcepath src src/nao/atlasengine/*.java
	java -cp bin nao.atlasengine.Game

compile:
	javac -d bin -sourcepath src src/org/atlasengine/*.java
	javac -d bin -sourcepath src src/org/enginetest/*.java

run:
	java -cp bin org.enginetest.Game

vim:
	vim src/org/

gvim:
	gvim src/org/

raffa:
	javac -d bin -sourcepath src src/raffa/atlasenginie/*.java
	javac -d bin -sourcepath src src/raffa/test/*.java
	java -cp bin raffa.test.Game
